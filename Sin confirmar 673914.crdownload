def analizar_transacciones():
    # Simulaci√≥n de transacciones (tipo, monto)
    transacciones = [
        {"Tipo": "Ingreso", "Monto": 1500},
        {"Tipo": "Egreso", "Monto": 500},
        {"Tipo": "Ingreso", "Monto": 800},
        {"Tipo": "Egreso", "Monto": 300},
    ]

    ingresos = 0
    egresos = 0

    for fila in transacciones:
        monto = fila["Monto"]
        if fila["Tipo"] == "Ingreso":
            ingresos += monto
        elif fila["Tipo"] == "Egreso":
            egresos += monto

    saldo = ingresos - egresos

    print("\nüìä Resumen de transacciones:")
    print(f"‚úîÔ∏è Total ingresos: {ingresos}")
    print(f"‚ùå Total egresos: {egresos}")
    print(f"üí∞ Saldo final: {saldo}")

# Ejecutar la funci√≥n
analizar_transacciones()
